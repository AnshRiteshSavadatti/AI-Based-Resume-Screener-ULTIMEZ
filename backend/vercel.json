{
  "version": 2,
  "builds": [
    {
      "src": "backend/api/server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "backend/api/server.js"
    }
  ],
  "functions": {
    "backend/api/server.js": {
      "includeFiles": [
        "backend/api/config/**",
        "backend/api/controller/**",
        "backend/api/database/**",
        "backend/api/models/**",
        "backend/api/routes/**",
        "backend/api/.env.development.local"
      ]
    }
  }
}
